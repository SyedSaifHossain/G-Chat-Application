<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/splashScreenFragment">

    <fragment
        android:id="@+id/welcomeFragment"
        android:name="com.syedsaifhossain.g_chatapplication.WelcomeFragment"
        android:label="fragment_welcome"
        tools:layout="@layout/fragment_welcome" >
        <action
            android:id="@+id/welcomeFragmentTosignUpFragment"
            app:destination="@id/signUpFragment" />
        <action
            android:id="@+id/welcomeFragmentToSignIn"
            app:destination="@id/signInFragment" />
    </fragment>
    <fragment
        android:id="@+id/signUpFragment"
        android:name="com.syedsaifhossain.g_chatapplication.SignUpFragment"
        android:label="fragment_sign_up"
        tools:layout="@layout/fragment_sign_up" >

        <action
            android:id="@+id/signUpToSignupNext"
            app:destination="@id/signupNextFragment" />
        <action
            android:id="@+id/action_signUpFragment_to_selectRegionFragment"
            app:destination="@id/selectRegionFragment" />
    </fragment>
    <fragment
        android:id="@+id/homeFragment"
        android:name="com.syedsaifhossain.g_chatapplication.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_homeFragment_to_newChatsFragment"
            app:destination="@id/newChatsFragment" />
        <action
            android:id="@+id/action_homeFragment_to_addContactsFragment"
            app:destination="@id/addContactsFragment" />
        <action
            android:id="@+id/action_homeFragment_to_scanFragment"
            app:destination="@id/scanFragment" />
    </fragment>
    <fragment
        android:id="@+id/signInFragment"
        android:name="com.syedsaifhossain.g_chatapplication.SignInFragment"
        android:label="fragment_sign_in"
        tools:layout="@layout/fragment_sign_in" >
        <action
            android:id="@+id/signInFragment_to_signInNextFragment"
            app:destination="@id/signInNextFragment" />
        <action
            android:id="@+id/action_signInFragment_to_selectRegionFragment"
            app:destination="@id/selectRegionFragment" />
        <action
            android:id="@+id/action_signInFragment_to_loginViaEmailFragment"
            app:destination="@id/loginViaEmailFragment" />
    </fragment>
    <fragment
        android:id="@+id/signupNextFragment"
        android:name="com.syedsaifhossain.g_chatapplication.SignupNextFragment"
        android:label="fragment_signup_next"
        tools:layout="@layout/fragment_signup_next" >
        <action
            android:id="@+id/signupNextFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
    </fragment>
    <fragment
        android:id="@+id/signInNextFragment"
        android:name="com.syedsaifhossain.g_chatapplication.SignInNextFragment"
        android:label="fragment_sign_in_next"
        tools:layout="@layout/fragment_sign_in_next" >
        <action
            android:id="@+id/signInNextFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
        <action
            android:id="@+id/action_signInNextFragment_to_loginViaPhoneFragment"
            app:destination="@id/loginViaPhoneFragment" />
    </fragment>
    <fragment
        android:id="@+id/selectRegionFragment"
        android:name="com.syedsaifhossain.g_chatapplication.SelectRegionFragment"
        android:label="fragment_select_region"
        tools:layout="@layout/fragment_select_region" />
    <fragment
        android:id="@+id/loginViaEmailFragment"
        android:name="com.syedsaifhossain.g_chatapplication.LoginViaEmailFragment"
        android:label="fragment_login_via_email"
        tools:layout="@layout/fragment_login_via_email" >
        <action
            android:id="@+id/action_loginViaEmailFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
    </fragment>
    <fragment
        android:id="@+id/loginViaPhoneFragment"
        android:name="com.syedsaifhossain.g_chatapplication.LoginViaPhoneFragment"
        android:label="fragment_login_via_phone"
        tools:layout="@layout/fragment_login_via_phone" >
        <action
            android:id="@+id/action_loginViaPhoneFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
    </fragment>
    <fragment
        android:id="@+id/chatFragment5"
        android:name="com.syedsaifhossain.g_chatapplication.ChatFragment"
        android:label="fragment_chat"
        tools:layout="@layout/fragment_chat" />
    <fragment
        android:id="@+id/meFragment3"
        android:name="com.syedsaifhossain.g_chatapplication.MeFragment"
        android:label="fragment_me"
        tools:layout="@layout/fragment_me" />
    <fragment
        android:id="@+id/contactFragment3"
        android:name="com.syedsaifhossain.g_chatapplication.ContactFragment"
        android:label="fragment_contact"
        tools:layout="@layout/fragment_contact" />
    <fragment
        android:id="@+id/discoverFragment3"
        android:name="com.syedsaifhossain.g_chatapplication.DiscoverFragment"
        android:label="fragment_discover"
        tools:layout="@layout/fragment_discover" />
    <fragment
        android:id="@+id/newChatsFragment"
        android:name="com.syedsaifhossain.g_chatapplication.NewChatsFragment"
        android:label="fragment_new_chats"
        tools:layout="@layout/fragment_new_chats" >
        <action
            android:id="@+id/action_newChatsFragment_to_selectGroupFragment"
            app:destination="@id/selectGroupFragment" />
    </fragment>
    <fragment
        android:id="@+id/addContactsFragment"
        android:name="com.syedsaifhossain.g_chatapplication.AddContactsFragment"
        android:label="fragment_add_contacts"
        tools:layout="@layout/fragment_add_contacts" />
    <fragment
        android:id="@+id/scanFragment"
        android:name="com.syedsaifhossain.g_chatapplication.ScanFragment"
        android:label="fragment_scan"
        tools:layout="@layout/fragment_scan" />
    <fragment
        android:id="@+id/selectGroupFragment"
        android:name="com.syedsaifhossain.g_chatapplication.SelectGroupFragment"
        android:label="fragment_select_group"
        tools:layout="@layout/fragment_select_group" />
    <fragment
        android:id="@+id/splashScreenFragment"
        android:name="com.syedsaifhossain.g_chatapplication.SplashScreenFragment"
        android:label="fragment_splash_screen"
        tools:layout="@layout/fragment_splash_screen" >
        <action
            android:id="@+id/action_splashScreenFragment_to_welcomeFragment"
            app:destination="@id/welcomeFragment" />
    </fragment>
</navigation>